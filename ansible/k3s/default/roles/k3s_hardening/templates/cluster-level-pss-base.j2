apiVersion: apiserver.config.k8s.io/v1
kind: AdmissionConfiguration
plugins:
- name: PodSecurity
  configuration:
    apiVersion: pod-security.admission.config.k8s.io/v1beta1
    kind: PodSecurityConfiguration
    defaults:
      enforce: "restricted"
      enforce-version: "latest"
      audit: "restricted"
      audit-version: "latest"
      warn: "restricted"
      warn-version: "latest"
    exemptions:
      usernames: []
      runtimeClasses: []
      namespaces: [kube-system, cis-operator-system, cattle-system, cattle-epinio-system, cattle-fleet-system, longhorn-system, cattle-neuvector-system, cattle-monitoring-system, rancher-alerting-drivers, cattle-logging-system, cattle-windows-gmsa-system, cattle-gatekeeper-system, istio-system, cattle-istio-system, cattle-istio, kiali-operator, cattle-ui-plugin-system]
